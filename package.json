{
  "name": "@istanbul/cors",
  "version": "0.0.2",
  "description": "Cors plugin of istanbul framework",
  "homepage": "https://github.com/istanbulnode/cors#readme",
  "keywords": [
    "typescript",
    "istanbul",
    "cors",
    "allow-origin"
  ],
  "author": "Sami Salih İBRAHİMBAŞ",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istanbulnode/cors.git"
  },
  "bugs": {
    "url": "https://github.com/istanbulnode/cors/issues"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --watchAll --no-cache",
    "clear": "del-cli ./dist/*",
    "build": "npm run clear && tsc"
  },
  "dependencies": {
    "@istanbul/app": "^0.0.27",
    "@istanbul/core": "^0.0.8",
    "vary": "^1.1.2"
  },
  "devDependencies": {
    "@istanbul/http": "^0.0.20",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@types/vary": "^1.1.0",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/$1",
      "~/(.*)": "<rootDir>/$1"
    }
  }
}
